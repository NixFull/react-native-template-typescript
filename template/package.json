{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update-dev-dependencies": "yarn add -D @types/fbemitter @types/react babel-plugin-module-resolver eslint-import-resolver-babel-module eslint-plugin-import eslint-plugin-jest eslint-plugin-jsx-a11y eslint-plugin-react identity-obj-proxy",
    "update-dependencies": "yarn add @react-native-async-storage/async-storage react-native-paper @react-navigation/native @react-navigation/native-stack react-native-safe-area-context react-native-screens",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "prettier-check": "node_modules/.bin/prettier ./.prettierrc.js --check '{app,__tests__}/**/*.js'",
    "prettier-run": "node_modules/.bin/prettier ./.prettierrc.js --write \"{app,__tests__}/**/*.js\"",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "android-dev-win": "SET ENVFILE=.env.dev && react-native run-android",
    "android-prod": "ENVFILE=.env.production react-native run-android",
    "apk-prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "apk-prod-win": "cd android && SET ENVFILE=.env.production ./gradlew assembleRelease",
    "apk-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease",
    "bundle-dev": "cd android && ENVFILE=.env.dev ./gradlew bundleRelease",
    "bundle-prod": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios",
    "ios-prod": "ENVFILE=.env.production react-native run-ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "i18next": "^22.0.4",
    "react": "18.1.0",
    "react-i18next": "^12.0.0",
    "react-native": "0.70.5",
    "react-native-paper": "^4.12.5",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
